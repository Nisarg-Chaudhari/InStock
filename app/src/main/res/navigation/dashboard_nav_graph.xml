    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_nav_graph"
    app:startDestination="@id/catagoriesFragment">

    <fragment
        android:id="@+id/catagoriesFragment"
        android:name="com.example.instock.CatagoriesFragment"
        android:label="fragment_catagories"
        tools:layout="@layout/fragment_catagories" >
        <action
